    "find-root": "^0.1.1",
    "git-validate": "^2.1.0",
    "inquirer": "^0.10.0",
    "is-email": "^0.1.1",
    "jsonwebtoken": "^5.0.5",
    "minimist": "^1.2.0",
    "rc": "^1.1.1",
    "requiresafe-sdk": "^3.0.0",
    "semver": "^5.0.3"
  },
  "devDependencies": {
    "code": "^1.5.0",
