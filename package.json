{
  "name": "requiresafe",
  "description": "the requiresafe.com command line interface tool",
  "version": "1.5.0",
  "author": "^lift security",
  "bin": {
    "requiresafe": "bin/requiresafe"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "cli-table": "^0.3.1",
    "git-validate": "^2.1.0",
    "joi": "^6.8.1",
    "subcommand": "^2.0.3",
    "wreck": "^6.3.0"
  },
  "devDependencies": {
    "code": "^1.5.0",
    "lab": "^6.1.0",
    "shrinkydink": "^1.0.0"
  },
  "keywords": [
    "security",
    "requiresafe"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/requiresafe/cli.git"
  },
  "scripts": {
    "configure-local": "node -e \"var uc = require('./lib/update-config'); uc('local', true, process.exit)\"",
    "setup-offline": "curl -sS https://api.requiresafe.com/api/v1/advisories -o advisories.json",
    "test": "lab -a code -L",
    "shrinkwrap": "npm shrinkwrap && shrinkydink"
  },
  "pre-commit": [
    "test"
  ]
}
